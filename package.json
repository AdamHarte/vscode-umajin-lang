{
    "name": "umajin",
    "displayName": "Umajin",
    "description": "Provide Umajin language support for Visual Studio Code.",
    "version": "0.0.1",
    "publisher": "adamharte",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "umajin",
            "aliases": ["Umajin", "umajin"],
            "extensions": [".u"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "umajin",
            "scopeName": "source.umajin",
            "path": "./syntaxes/umajin.tmLanguage.json"
        }],
        "snippets": [{
            "language": "umajin",
            "path": "./snippets/umajin.json"
        }]
    }
}